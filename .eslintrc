{
  "plugins": [
    "prettier",
    "import"
  ],
  "extends": [
    "plugin:prettier/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-debugger": "warn",
    "no-console": "warn"
  }
}